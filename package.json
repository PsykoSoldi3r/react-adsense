{
  "name": "@ctrl/react-adsense",
  "version": "0.0.0-placeholder",
  "description": "Typescript adsense module",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "scttcper/react-adsense",
  "main": "dist/index.js",
  "module": "dist/module/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "keywords": [
    "typescript"
  ],
  "scripts": {
    "lint": "eslint --ext .js,.ts, .",
    "lint:fix": "eslint --fix --ext .js,.ts, .",
    "prepare": "npm run build",
    "build": "tsc -p tsconfig.build.json && tsc -p tsconfig.module.json",
    "watch:demo": "parcel serve demo/index.html --open",
    "build:demo": "parcel build demo/index.html",
    "test": "jest",
    "test:update": "jest --update-snapshot",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit --coverage"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.0",
    "@babel/plugin-transform-runtime": "^7.12.0",
    "@babel/preset-react": "^7.10.4",
    "@ctrl/eslint-config": "1.2.4",
    "@fullhuman/postcss-purgecss": "3.0.0",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.1.0",
    "@types/jest": "26.0.14",
    "@types/node": "14.11.8",
    "@types/react": "16.9.52",
    "autoprefixer": "9.8.6",
    "eslint-config-xo-react": "0.23.0",
    "eslint-plugin-react": "7.21.4",
    "eslint-plugin-react-hooks": "4.1.2",
    "jest": "26.5.3",
    "jest-junit": "12.0.0",
    "parcel": "1.12.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "tailwindcss": "1.9.2",
    "ts-jest": "26.4.1",
    "typescript": "4.0.3"
  },
  "jest": {
    "preset": "ts-jest",
    "roots": [
      "<rootDir>/src"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branch": "master"
  }
}
