{
  "name": "@ctrl/react-adsense",
  "version": "0.0.0-placeholder",
  "description": "Typescript adsense module",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "scttcper/react-adsense",
  "main": "dist/index.js",
  "module": "dist/module/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "keywords": [
    "typescript"
  ],
  "scripts": {
    "lint": "eslint --ext .js,.ts, .",
    "lint:fix": "eslint --fix --ext .js,.ts, .",
    "prepare": "npm run build",
    "build": "tsc -p tsconfig.build.json && tsc -p tsconfig.module.json",
    "watch:demo": "parcel serve demo/index.html --open",
    "build:demo": "parcel build demo/index.html",
    "test": "jest",
    "test:update": "jest --update-snapshot",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit --coverage"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/plugin-transform-runtime": "7.12.1",
    "@babel/preset-react": "7.12.1",
    "@ctrl/eslint-config": "1.2.5",
    "@fullhuman/postcss-purgecss": "3.0.0",
    "@testing-library/jest-dom": "5.11.5",
    "@testing-library/react": "11.1.0",
    "@types/jest": "26.0.15",
    "@types/node": "14.14.6",
    "@types/react": "16.9.55",
    "autoprefixer": "10.0.1",
    "eslint-config-xo-react": "0.23.0",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "jest": "26.6.1",
    "jest-junit": "12.0.0",
    "parcel": "1.12.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "tailwindcss": "1.9.6",
    "ts-jest": "26.4.3",
    "typescript": "4.0.5"
  },
  "jest": {
    "preset": "ts-jest",
    "roots": [
      "<rootDir>/src"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branch": "master"
  }
}
