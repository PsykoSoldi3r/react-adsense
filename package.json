{
  "name": "@ctrl/react-adsense",
  "version": "0.0.0-placeholder",
  "description": "Typescript adsense module",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "scttcper/react-adsense",
  "main": "dist/index.js",
  "module": "dist/module/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "keywords": [
    "typescript"
  ],
  "scripts": {
    "lint": "eslint --ext .js,.ts, .",
    "lint:fix": "eslint --fix --ext .js,.ts, .",
    "prepare": "npm run build",
    "build": "tsc -p tsconfig.build.json && tsc -p tsconfig.module.json",
    "demo:watch": "snowpack dev",
    "demo:build": "snowpack build",
    "test": "jest",
    "test:update": "jest --update-snapshot",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit --coverage"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/plugin-transform-runtime": "7.12.10",
    "@babel/preset-react": "7.12.10",
    "@ctrl/eslint-config": "1.2.11",
    "@fullhuman/postcss-purgecss": "4.0.0",
    "@snowpack/plugin-postcss": "1.1.0",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.3",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.22",
    "@types/react": "17.0.0",
    "autoprefixer": "10.2.3",
    "cssnano": "4.1.10",
    "eslint-config-xo-react": "0.23.0",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "jest": "26.6.3",
    "jest-junit": "12.0.0",
    "postcss": "8.2.4",
    "postcss-cli": "8.3.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "snowpack": "3.0.11",
    "tailwindcss": "2.0.2",
    "ts-jest": "26.4.4",
    "typescript": "4.1.3"
  },
  "jest": {
    "preset": "ts-jest",
    "roots": [
      "<rootDir>/src"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branch": "master"
  }
}
